# AUMA GHL App - Environment Variables Template
# Copy this file to .env and fill in the values

# =============================================================================
# GHL OAuth (Required)
# =============================================================================
GHL_CLIENT_ID=your_marketplace_app_client_id
GHL_CLIENT_SECRET=your_marketplace_app_client_secret
GHL_REDIRECT_URI=https://api.launchmaniac.com/auth/ghl/callback
GHL_APP_ID=your_app_id

# =============================================================================
# Deepseek AI (Required)
# =============================================================================
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_MODEL=deepseek-chat-v3.2
DEEPSEEK_BASE_URL=https://api.deepseek.com

# =============================================================================
# Supabase (Required - Self-hosted)
# =============================================================================
SUPABASE_URL=https://supabase.launchmaniac.com
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_key

# =============================================================================
# Redis (Required)
# =============================================================================
REDIS_URL=redis://redis.launchmaniac.com:6379

# =============================================================================
# Portal Configuration
# =============================================================================
PORTAL_URL=https://portal.launchmaniac.com

# =============================================================================
# Email/Notifications (Required)
# =============================================================================
SMTP2GO_API_KEY=your_smtp2go_key
SMTP2GO_SENDER=noreply@launchmaniac.com

# =============================================================================
# Cloudflare for Customers (Enterprise white-label)
# =============================================================================
CLOUDFLARE_ACCOUNT_ID=your_account_id
CLOUDFLARE_API_TOKEN=your_api_token
CLOUDFLARE_ZONE_TEMPLATE_ID=your_template_zone_id

# =============================================================================
# Security
# =============================================================================
ENCRYPTION_KEY=32_byte_hex_key_for_token_encryption
JWT_SECRET=your_jwt_secret_key

# =============================================================================
# Environment
# =============================================================================
NODE_ENV=development
PORT=3000
